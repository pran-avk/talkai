{
    "builds": [
      {
        "src": "core/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.12"
        }
      }
    ],
    "functions": {
      "api/my-function.js": {
        "memory": 512,
        "maxDuration": 10,
        "excludeFiles": "**/*.md"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "core/wsgi.py"
      }
    ]
  }
  